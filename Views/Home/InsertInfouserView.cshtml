<!-- Custom CSS -->
<div class="table-responsive">
<table class="table color-table inverse-table" >

    <tr>
        <th>Distrito Metroppolitanino de Quito, </th>
        <th>@DateTime.Now.ToString()</th>
        
        
        <th></th>
    </tr>
  
</table>
