@using System.Data
@model DataTable
@{
    ViewData["Title"] = "UserView";
 }



<div class="row">
    <div class="col-md-12">
        <div class="panel">
            <div class="panel-heading">Manejo de Pacientes</div>
            <div class="table-responsive">
                <table class="table table-hover manage-u-table">
                    <thead>
                        <tr>
                            <th>Historia clinica</th>
                            <th>Apellidos </th>
                            <th>Nombres</th>
                            <th>Genero</th>
                            <th>Edad</th>
                            <th>Fecha de Nacimiento</th>
                            <th>Direccion</th>
                            <th>Telefono</th>
                            <th>Instruccion</th>
                            <th>Institucion</th>
                            <th>Fuente de Informacion</th>
                            <th>Email</th>
                            <th>Fecha entrega Informe</th>
                        </tr>
                    </thead>
                      @foreach (DataRow row in @Model.Rows)
                                        {
                    <tbody>
                            <tr class="advance-table-row ">
                                <td width="10"> @row["pac_HistorialClinico"]</td>
                                <td width="40">
                                    @row["pac_apellidos"]
                                   
                                </td>
                                <td>@row["pac_genero"]</td>
                                <td>@row["pac_edad"] </td>
                                <td>@row["pac_fnacimiento"]</td>
                                <td>@row["pac_direccion"]</td>
                                <td>@row["pac_telefono"]</td>
                                <td>@row["pac_instruccion"]</td>
                                <td>@row["pac_institucion"]</td>
                                <td>@row["pac_finformacion"]</td>
                                <td>@row["pac_email"]</td>
                                <td>@row["pac_fentregainforme"]</td>
                                <td>
                                    <button type="button" class="btn btn-info btn-outline btn-circle btn-lg m-r-5"><i class="icon-trash"></i></button>
                                    <button type="button" class="btn btn-info btn-outline btn-circle btn-lg m-r-5"><i class="ti-pencil-alt"></i></button>
                                            </td>
                            </tr>


                            <tr>
                                <td colspan="7" class="sm-pd"></td>
                            </tr>
                       
                        
                    </tbody>
                    }
                </table>
            </div>
        </div>
    </div>
</div>